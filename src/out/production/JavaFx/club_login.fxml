<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300" prefWidth="400" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
   <children>
      <Label layoutX="30" layoutY="30" text="Club Name:" />
      <TextField fx:id="clubNameField" layoutX="137.0" layoutY="25" prefHeight="26.0" prefWidth="213.0" />
      <Label layoutX="30" layoutY="80" text="Password:" />
      <PasswordField fx:id="passwordField" layoutX="137.0" layoutY="76.0" prefHeight="26.0" prefWidth="203.0" />
      <Button fx:id="registerButton" layoutX="170.0" layoutY="181.0" onAction="#handleLogin" prefHeight="26.0" prefWidth="70.0" text="Login" />
      <Label fx:id="statusLabel" layoutX="30" layoutY="230" prefWidth="350" textFill="red" />
   </children>
</AnchorPane>
